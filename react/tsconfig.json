{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "components/*": ["./src/components/*"],
      "logos/*": ["./public/logos/*"],
      "iconos/*": ["./public/iconos/*"],
      "consultas/*": ["./src/consultas/*"],
      "estilos_paginas/*": ["./src/pages/estilos/*"],
      "bases_de_datos/*": ["./src/bases_de_datos/*"],
      "buscadores/*": ["./src/buscadores/*"],
      "estilosParaConsultaConMapas/*": ["./src/components/consultaConMapa/estilos.ts"],
      "utilidades/*": ["./src/utilidades/*"],
      "tipos/*": ["./src/tipos/*"],
      "estilosParaMapas/*": ["./src/estilos/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}